{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "lazy",
      "updateMode": "lazy",
      "resources": {
        "files": ["/index.html"]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "google-fonts",
      "urls": [
        "https://fonts.googleapis.com/**",
        "https://fonts.gstatic.com/**"
      ],
      "cacheConfig": {
        "strategy": "freshness",
        "maxAge": "0s",
        "maxSize": 0
      }
    }
  ]
}